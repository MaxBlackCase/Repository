<Window x:Class="Dates_Calendars.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dates_Calendars"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="500">
    <Grid>
    <Calendar x:Name="cal1"
              FirstDayOfWeek="Monday"
              SelectedDatesChanged="calendar_SelectedChanged"
              HorizontalAlignment="Left">

      <Calendar.BlackoutDates>
        <CalendarDateRange Start="6/1/2020"
                           End="6/10/2020"></CalendarDateRange>
      </Calendar.BlackoutDates>
            
    </Calendar>
    </Grid>
</Window>
